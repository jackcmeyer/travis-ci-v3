{
  "name": "travis-ci-v3",
  "version": "0.0.0",
  "description": "An npm package which wraps the TravisCI v3 API",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsc src/index.ts",
    "build": "tsc -p . --outDir ./dist",
    "pretest": "tsc -p .",
    "test": "nyc mocha -r ts-node/register test/**/*.spec.ts",
    "posttest": "nyc report --reporter=json && codecov -f coverage/coverage-final.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackcmeyer/travis-ci-v3.git"
  },
  "keywords": [
    "TravisCI"
  ],
  "author": "Jack Meyer <jackcmeyer@gmail.com> (https://jackcmeyer.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jackcmeyer/travis-ci-v3/issues"
  },
  "homepage": "https://github.com/jackcmeyer/travis-ci-v3#readme",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/nock": "^9.3.0",
    "@types/sinon": "^5.0.1",
    "chai": "^4.1.2",
    "codecov": "^3.0.4",
    "mocha": "^5.2.0",
    "nock": "^9.6.1",
    "nyc": "^12.0.2",
    "sinon": "^6.1.5",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "@types/got": "^8.3.3",
    "@types/node": "^10.7.1",
    "@types/node-fetch": "^2.1.2",
    "got": "^9.0.0",
    "node-fetch": "^2.2.0"
  }
}
